Host github.com-private
HostName github.com
AddKeysToAgent yes
User git
IdentityFile ~/.ssh/private_rsa
IdentitiesOnly yes

Host localhost
HostName 127.0.0.1
Port 22
User pair
IdentityFile ~/.ssh/private_rsa
StrictHostKeyChecking no

Host bitbucket.nentgroup.com
AddKeysToAgent yes
User git
IdentityFile ~/.ssh/nentgroup_rsa
IdentitiesOnly yes
